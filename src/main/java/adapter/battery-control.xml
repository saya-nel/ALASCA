<control-adapter
        type="planning"
        uid="1A10000"
        offered="main.java.interfaces.BatteryCI">
    <consumption nominal="200"/>
    <upMode>
        <required>main.java.interfaces.BatteryCI</required>
        <body equipmentRef="battery">
            return battery.upMode();
        </body>
    </upMode>
    <downMode>
	<required>main.java.interfaces.BatteryCI</required>
        <body equipmentRef="battery">
	    return battery.downMode();
	</body>
    </downMode>
</control-adapter>
